@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Real Estate Admin</title>
    <link rel="stylesheet" href="~/Content/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/owl-carousel-2/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/style.css">
    <link rel="shortcut icon" href="~/Content/assets/images/favicon.png" />
</head>
<body>
    <div class="container-scroller">
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
            <ul class="nav">
                <li class="nav-item profile">
                    <div class="profile-desc">
                        <div class="profile-pic">
                            <div class="count-indicator">
                                <img class="img-xs rounded-circle " src="~/Content/assets/images/faces/face15.jpg" alt="">
                                <span class="count bg-success"></span>
                            </div>
                            <div class="profile-name">
                                <h5 class="mb-0 font-weight-normal">Doğan Berk Bulur</h5>
                                <span>Yönetici</span>
                            </div>
                        </div>
                        <a href="#" id="profile-dropdown" data-toggle="dropdown"><i class="mdi mdi-dots-vertical"></i></a>
                        <div class="dropdown-menu dropdown-menu-right sidebar-dropdown preview-list" aria-labelledby="profile-dropdown">
                            <a href="#" class="dropdown-item preview-item">
                                <div class="preview-thumbnail">
                                    <div class="preview-icon bg-dark rounded-circle">
                                        <i class="mdi mdi-settings text-primary"></i>
                                    </div>
                                </div>
                                <div class="preview-item-content">
                                    <p class="preview-subject ellipsis mb-1 text-small">Hesap Ayarları</p>
                                </div>
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="#" class="dropdown-item preview-item">
                                <div class="preview-thumbnail">
                                    <div class="preview-icon bg-dark rounded-circle">
                                        <i class="mdi mdi-onepassword  text-info"></i>
                                    </div>
                                </div>
                                <div class="preview-item-content">
                                    <p class="preview-subject ellipsis mb-1 text-small">Şifre Değiştir</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </li>
                <li class="nav-item nav-category">
                    <span class="nav-link">MENÜ</span>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="#">
                        <span class="menu-icon">
                            <i class="mdi mdi-speedometer"></i>
                        </span>
                        <span class="menu-title">Anasayfa</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="~/Haber/AdminHaberListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-playlist-play"></i>
                        </span>
                        <span class="menu-title">Haberler</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="~/Hakkinda/AdminHakkindaListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-table-large"></i>
                        </span>
                        <span class="menu-title">Hakkında</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="~/Hizmet/AdminHizmetListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-chart-bar"></i>
                        </span>
                        <span class="menu-title">Hizmetler</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="~/Ilan/AdminIlanListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-contacts"></i>
                        </span>
                        <span class="menu-title">İlanlar</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="~/Kategori/AdminKategoriListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-file-document-box"></i>
                        </span>
                        <span class="menu-title">Kategoriler</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="~/KonutTipi/AdminKonutTipiListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-chart-bar"></i>
                        </span>
                        <span class="menu-title">Konut Tipleri</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="/Kullanici/AdminKullaniciListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-speedometer"></i>
                        </span>
                        <span class="menu-title">Kullanıcılar</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="/MusteriMesaj/AdminMusteriMesajListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-speedometer"></i>
                        </span>
                        <span class="menu-title">Müşteri Mesajları</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="/MusteriYorumlari/AdminMusteriYorumListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-speedometer"></i>
                        </span>
                        <span class="menu-title">Müşteri Yorumları</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="/Sehir/AdminSehirListeleme">
                        <span class="menu-icon">
                            <i class="mdi mdi-speedometer"></i>
                        </span>
                        <span class="menu-title">Şehirler</span>
                    </a>
                </li>
            </ul>
        </nav>
        <div class="container-fluid page-body-wrapper">
            <nav class="navbar p-0 fixed-top d-flex flex-row">
                <div class="navbar-brand-wrapper d-flex d-lg-none align-items-center justify-content-center">
                    <a class="navbar-brand brand-logo-mini" href="index.html"><img src="~/Content/assets/images/logo.png" alt="logo" /></a>
                </div>
                <div class="navbar-menu-wrapper flex-grow d-flex align-items-stretch">
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-menu"></span>
                    </button>
                    <ul class="navbar-nav w-100">
                        <li class="nav-item w-100">
                            <div class="nav-link mt-2 mt-md-0 d-none d-lg-flex search">
                                <input type="text" class="form-control" placeholder="Şarkı ara ...">
                                <input type="submit" value="ARA" style="color:white" />
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright © Doğan Berk Bulur || 2023</span>
                    </div>
                </footer>
            </div>
        </div>
    </div>
    <script src="~/Content/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/Content/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/Content/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/Content/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/Content/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Content/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <script src="~/Content/assets/js/off-canvas.js"></script>
    <script src="~/Content/assets/js/hoverable-collapse.js"></script>
    <script src="~/Content/assets/js/misc.js"></script>
    <script src="~/Content/assets/js/settings.js"></script>
    <script src="~/Content/assets/js/todolist.js"></script>
    <script src="~/Content/assets/js/dashboard.js"></script>
</body>
</html>

